    "@babel/register": "^7.9.0",
    "@pubcore/ui-text": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
